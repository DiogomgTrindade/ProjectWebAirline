@model IEnumerable<ProjectWebAirlineMVC.Data.Entities.User>

@{
    ViewData["Title"] = "UsersList";
}

<h1>Users</h1>

<table class="table">
    <thead>
        <tr>
            <th>Full Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.FullName</td>
                <td>@user.Email</td>
            </tr>
        }
    </tbody>
</table>
